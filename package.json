{
  "name": "fgc-kenya",
  "version": "1.0.0",
  "description": "Official website for FIRST Global Team Kenya - inspiring young innovators through robotics and STEM education",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "dev:clean": "./start-dev.sh",
    "dev:fast": "pkill -9 node 2>/dev/null; pkill -9 npm 2>/dev/null; rm -rf .next; next dev",
    "dev:next": "next dev",
    "dev:full": "echo 'ðŸš€ Run these commands in separate terminals:' && echo '  Terminal 1: npm run dev:next' && echo '  Terminal 2: npm run maildev:dev' && echo '' && echo 'Services:' && echo '  â€¢ App: http://localhost:3000' && echo '  â€¢ Email: http://localhost:1080' && echo '  â€¢ Admin: admin@example.com (check email)'",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "server": "node server/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "export": "next export",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "maildev": "maildev",
    "maildev:dev": "maildev --web 1080 --smtp 1025 --web-ip 127.0.0.1 --smtp-ip 127.0.0.1",
    "setup": "npm install && npx prisma generate && npx prisma db push && npm run db:seed",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "security:check": "npm audit --audit-level=moderate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "FIRST Global",
    "Team Kenya",
    "Robotics",
    "STEM",
    "Education",
    "Kenya",
    "Next.js",
    "React",
    "TypeScript"
